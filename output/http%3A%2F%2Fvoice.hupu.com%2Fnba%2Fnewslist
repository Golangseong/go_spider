<!-- voice config 配置 -->
<script>
!function(HP){
    var win = window,
        util = HP.util;

    win.HP = win.HP || {};
    /**
     * Voice 命名空间
     */
    var Voice = HP.Voice = {};

    /**
     * 接口地址
     */ 
    Voice.apiUrl = {
                // （内容）支持，反对
        support : 'http://voice.hupu.com/message_support_agaist',
        // 话题
        object : {
            // 关注话题
            follow : 'http://voice.hupu.com/api/userFollowObject',
            // 获取话题信息
            getObjectInfo : 'http://voice.hupu.com/api/get_object_info'
        },
        // 评论
        comment: {
            // 支持，反对
            support : '/support_agaist',
            // 回复评论
            reply : '/stars/xhrreply'
        },
        // my接口地址
        my: {
            // people 更新用户信息
            updatePeopleInfo: '/update_people_information',
            // people页 加载更多
            morePeopleMsg: 'http://voice.hupu.com/get_more_people_messages'
        },
        // 直播专题
        liveTopic: {
            moreMessages : '/index/getMoreLiveTopicMessages',
            latestMessages : '/index/getLiveTopicMessages'
        },
        fixed: {
        // 新版吐槽
            feedback: '/index/feedback'
        }
    }

    // 用户id
    Voice.getUserId = 0;
    // 用户名称
    Voice.getUserName = '';
    // 用户论坛等级
    Voice.getUserLevel = '';

    Voice.getUserExtra = false;
    Voice.getUserExtra2 = eval('[]');

    // 为1时是论坛全站封禁用户，0时非封禁用户
    Voice.getUserBlackRoom = '';
    // 新声频道的禁言信息
    Voice.getUserBanned = '';
    // 新声页面名
    Voice.getPageName = 'newsList';
    // 新声频道名
    Voice.getProjectName = '';

    Voice.isLogin = util.isLogin();
    Voice.popLogin = function(){
        commonLogin('hupu');
    }

}(HP);
</script>

